<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="490.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.OrderDetailController.OrderDetailManagementFormController">
    <children>
        <AnchorPane layoutX="10.0" layoutY="10.0" prefHeight="381.0" prefWidth="509.0">
            <children>
                <Label fx:id="lblOrderManagementSystem" alignment="CENTER" layoutX="14.0" layoutY="14.0" prefHeight="38.0" prefWidth="495.0" text="Order Detail Management System">
                    <font>
                        <Font name="Bauhaus 93" size="26.0" />
                    </font>
                </Label>
                <TableView fx:id="tblOrderDetails" layoutX="34.0" layoutY="92.0" prefHeight="200.0" prefWidth="438.0">
                    <columns>
                        <TableColumn fx:id="colOrderId" prefWidth="78.0" text="Order ID" />
                        <TableColumn fx:id="colItemCode" prefWidth="87.0" text="Item Code" />
                        <TableColumn fx:id="colQuantity" prefWidth="88.0" text="Quantity" />
                        <TableColumn fx:id="colDiscount" prefWidth="91.0" text="Discount" />
                        <TableColumn fx:id="colTotal" prefWidth="91.0" text="Total" />
                    </columns>
                </TableView>
                <JFXButton fx:id="btnBackToDashboard" buttonType="RAISED" layoutX="183.0" layoutY="325.0" onAction="#btnBackToDashboardOnAction" text="Back to Dashboard" textFill="#9100b2">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </JFXButton>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
